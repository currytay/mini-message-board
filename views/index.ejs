<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <% messages.forEach(function(message) { %>
      <div>
        <p><%= message.user %></p>
        <p><%= message.text %></p>
        <p><%= message.added %></p>
      </div>
    <% }); %>
  </body>
</html>
